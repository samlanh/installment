function getCategoryByType(){
	cate_type = dijit.byId('cate_type').get('value');
	dijit.byId('income_category').reset(); 
	if(cate_type=="" || cate_type ==-1){
		income_category_store  = getDataStorefromJSON('id','name', <?php print_r(array())?>);
