var url_otherincome = '<?php echo $this->url(array('module'=>'incexp','controller'=>'incomeotherpayment','action'=>'getotherincomeinfo')); ?>';
function getRepairIncomeByID(){
	otherincome_id = dijit.byId('otherincome_id').get('value');
	if(otherincome_id=="" || otherincome_id ==-1){
		dijit.byId('balance').set('value',0); 
		dijit.byId('total_amount').set('value',0); 
		dijit.byId('remain').set('value',0); 
		return false;
	}
	dojo.xhrPost({
		url: url_otherincome,
		content:{
			'otherincome_id':otherincome_id
			},
		handleAs:"json",
		load: function(data) {
			dijit.byId('balance').set('value',data.total_amountafter); 
			dijit.byId('total_amount').set('value',data.total_amountafter); 
			dijit.byId('remain').set('value',0); 

			dijit.byId('customer_name').set('value',data.client_name); 
			dijit.byId('house_no').set('value',data.land_address);
			dijit.byId('street').set('value',data.street);
		},
		error: function(err) {
		}
	});
}

function getInvoiceNo(condition){
	cate_type = dijit.byId('cate_type').get('value');
	if(cate_type==12){
		var url_get_invoice = '<?php echo $this->url(array('module'=>'incexp','controller'=>'income','action'=>'get-invoice-no')); ?>';
	}else{
		var url_get_invoice = '<?php echo $this->url(array('module'=>'incexp','controller'=>'expense','action'=>'get-invoice-no')); ?>';
	}
	branch_id = dijit.byId('branch_id').get('value');
	if(branch_id==0){
		branch_id=1;
	}
	dojo.xhrPost({
		url:url_get_invoice,
		content:{
			'branch_id':branch_id
			},
		handleAs:"json",
		load: function(data) {
		    dijit.byId('invoice').attr('value',data);   
		    dojo.byId("lb_invoice").innerHTML = data;
		},
		error: function(err) {
		}
	});
}

function totalRemain(){
	var balance =	dijit.byId('balance').get('value'); 
	var total_amount = dijit.byId('total_amount').get('value');  

	if(balance<0){
		var remain = balance - total_amount;
		if(remain>0){
			dijit.byId('total_amount').set('value',balance); 
			remain=0;
		}
		dijit.byId('remain').set('value',remain); 
	}else{
		var remain = balance - total_amount;
		if(remain<0){
			dijit.byId('total_amount').set('value',balance); 
			remain=0;
		}
		dijit.byId('remain').set('value',remain); 
	}
}


function enablePayment(){
	payment_method = dijit.byId('payment_method').get('value');
	dijit.byId("cheque").set("readOnly",false);
	dijit.byId("cheque").attr("value",'');
	if(payment_method==1){
		dijit.byId("cheque").attr("value",'N/A');
		dijit.byId("cheque").set("readOnly",true);
	}
}
