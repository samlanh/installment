function typethisamename(index,type){
	description = dijit.byId('description_'+index).get('value');
	dijit.byId('remark_'+index).set('value',description);
}
function deleteRecords(index) {
	var identity = $('#identity').val();
	var arrays = identity.split(',');
	for(var i=0;i<arrays.length;i++) {
	if(arrays[i] == index) arrays.splice(i,1);
	}
	var strings = arrays.join(',');
	$('#identity').val(strings);
	dojo.query("#row"+index).remove();
	netTotal();
}	
function calculateTotal(index,type){
	price = dijit.byId('price_'+index).get('value');
	price = isNaN(price)?0:price;
	
	qty = dijit.byId('qty_'+index).get('value');
	qty = isNaN(qty)?0:qty;
	
	dijit.byId('total_'+index).attr('value',price * qty);
	
	dijit.byId('total_amount').attr('value',netTotal);	
	netTotal();
}
function netTotal(){ 
	var netTotal=0;
	var rowId = $('#identity').val(); //alert(1);
	var rowIDArray = rowId.split(',');
	for(var n = 0; n < rowIDArray.length; n++) {
		netTotal += dijit.byId('total_'+rowIDArray[n]).get('value');
	}
	dijit.byId('total_amount').attr('value',netTotal);
}


