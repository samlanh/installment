						if(isNaN(qtyAmt)){
							dijit.byId('qty_'+arrays[i]).attr('value',1);
						}
						sumAmountDue();
						HideloadingBlock();					 
						 return false;
					 }
				}
			}
	
		   HideloadingBlock();
		},
		error: function(err) {
			HideloadingBlock();
		}
	});
}

