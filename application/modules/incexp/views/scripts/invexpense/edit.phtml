		return false;
	}
	dojo.xhrPost({
		url:url_getgroup,
		content:{
				'pro_id':pro_id
				},
		handleAs:"json",
		load: function(data) {
		 	
			var iden = $("#identity").val();
			var arrays = iden.split(',');
			 if(arrays!=""){
				 for(var i=0;i< arrays.length;i++) {
					 readychoose = dijit.byId('product_name_'+arrays[i]).get('value');
