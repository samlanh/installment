}
function calCulateFirstPayment(){
	pay_term = 1;
	amount_date = 1;
	addamount=1;
	release_date = dijit.byId("release_date").get('value');
	var a = new Date(release_date);
		mm = a.getMonth()+amount_date+addamount;
	var dd = a.getDate();
	 if(dd<10){
		 dd = "0"+dd;
	}
	 if(mm<10){
		 mm = "0"+mm;
	}
	var y = a.getFullYear();
	var epx_date = y + '-'+ mm + '-'+dd ;
	dijit.byId('first_payment').attr('value',epx_date);
}
function calCulateEndDate(){
	amount_date = dijit.byId('period').get('value');
	release_date = dijit.byId("first_payment").get('value');//just update follow by first payment
	if(isNaN(amount_date)){
		dijit.byId('date_line').attr('value','<?php echo date("Y-m-d");?>');
	}else{
		var a = new Date(release_date);
		month = a.getMonth()+amount_date;
		mm = month%12;
		year = parseInt(month/12);
		 var dd = a.getDate();
		 if(dd<10){
			 dd = "0"+dd;
		}
		 if(mm<10){
			 mm = "0"+mm;
		}
		var y = a.getFullYear()+year;
		var epx_date = y + '-'+ mm + '-'+dd ;
		dijit.byId('date_line').attr('value',epx_date);	
    }
}
function calculatePayment(){
	sold_price = dijit.byId('sold_price').get('value');
	times_deposite = dijit.byId('times_deposite').get('value');
	if(isNaN(sold_price)){
		sold_price=0;
	}
	if(isNaN(times_deposite)){
		times_deposite=0;
	}
	var depositPayment = times_deposite*sold_price;
	dijit.byId('deposit').set('value',depositPayment);
	dijit.byId("deposit").set("readOnly",true);
	dijit.byId('balance').set('value',0);
	
}
