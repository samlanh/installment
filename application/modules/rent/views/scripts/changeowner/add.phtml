var url_rentinfo = "<?php echo $this->url(array('module'=>'rent','controller'=>'deposit','action'=>'getrentinfo')); ?>";
function getRentInfo(){
	rent_id = dijit.byId('rent_id').get('value');
	if(branch_id== 0 || branch_id==-1){
		return false;
	}
	dojo.xhrPost({
		url: url_rentinfo,	
		content:{ 
		    'rent_id':rent_id
		},		    
		handleAs:"json",
		load: function(data) {
			dijit.byId('from_customer').attr('value',data.client_id);
			dijit.byId("from_customer").set("readOnly",true);
