function checkPreScheduleOption(){
	payment_option = dijit.byId("pre_schedule_opt").get("value");
	if(payment_option==4){
		document.getElementById('schedule_step').style.display="none";
		document.getElementById('schedule_installment').style.display="block";

		$(".schedule_step").css("display", "none");
		$(".schedule_installment").css("display", "table-row");
		
		dijit.byId('pre_fix_payment').set('value',"");
	}else{
		document.getElementById('schedule_step').style.display="block";
		document.getElementById('schedule_installment').style.display="none";
		$(".schedule_step").css("display", "table-row");
		$(".schedule_installment").css("display", "none");
		
		dijit.byId('pre_percent_payment').set('value',"");
		dijit.byId('pre_percent_installment').set('value',"");
		dijit.byId('pre_amount_month').set('value',"");
		dijit.byId('pre_amount_year').set('value',"");
	}
}
