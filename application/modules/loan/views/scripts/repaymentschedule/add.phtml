function checkScheduleOption(){
	//document.getElementById('hidden_lastpayment').style.display='none';
	interest_rate = dijit.byId("interest_rate").get("value");
  	if(interest_rate==-1){
  		dijit.byId('frm_interest').show();
  		return false;
	}
	dijit.byId("install_type").set("readOnly",true);
	dijit.byId('first_payment').set("readOnly",false);
	dijit.byId('period').set("readOnly",false);
	dijit.byId('interest_rate').set("readOnly",false);
	dijit.byId('release_date').set("readOnly",false);
	dijit.byId('date_line').set("readOnly",true);
	method_id = dijit.byId('schedule_opt').get('value');

	amount_principal = dijit.byId('balance').get('value');	
	debt_amount = dijit.byId('period').get('value')*<?php echo $install_month;?>;
	if(method_id==3 || method_id==6){
		debt_amount = dijit.byId('period').get('value');
	}
	dojo.byId('lbl_period').innerHTML="<?php echo $tr->translate("TERM_BORROW")?>";
	dojo.byId('lbl_year').innerHTML="<?php echo ($install_month==12)?$tr->translate("YEAR"):$tr->translate("MONTH")?>";
	
	dojo.query("#table_row").append('');
	dijit.byId("total_installamount").attr("value",0);
	$('#identity').val('');
	dojo.query("#showrecord").append('');
	title=0;
	no=0;	
	if(method_id==4 || method_id==7){
			dijit.byId("install_type").set("readOnly",false);
			if(method_id==7 ){
				document.getElementById('hidden_lastpayment').style.display='flow-root';
			}
			calCulateFixedPeriod();	
		}else {
			dijit.byId('fixed_payment').attr('value',0);
			dijit.byId('fixed_payment').set("readOnly",true);
		if(method_id==1){//booking
			dijit.byId('date_line').attr('value','<?php echo date("Y-m-d");?>');
			dijit.byId('release_date').set("readOnly",true);
			dijit.byId('first_payment').set("readOnly",true);
			dijit.byId('date_line').set("readOnly",false);
			dijit.byId('period').set("readOnly",true);
			dijit.byId('period').attr("value",0);
			dijit.byId('interest_rate').attr("value",0);
			dijit.byId('interest_rate').set("readOnly",true);
		}else if(method_id==2){//payoff
			dijit.byId('release_date').set("readOnly",true);
			dijit.byId('first_payment').set("readOnly",true);
			dijit.byId('interest_rate').set("readOnly",true);
			dijit.byId('interest_rate').attr("value",0);
			dijit.byId('period').set("readOnly",true);
			amount_principal = dijit.byId("sold_price").get("value")-dijit.byId("paid_before").get("value");
			dijit.byId('deposit').attr("value",amount_principal);
			dijit.byId('balance').attr("value",0);
			dijit.byId('period').attr("value",0);
			dijit.byId('date_line').attr('value','<?php echo date("Y-m-d");?>');
			dijit.byId('first_payment').attr('value','<?php echo date("Y-m-d");?>');			
		}else if(method_id==3){//times
			dojo.byId('lbl_period').innerHTML="<?php echo $tr->translate("TOTAL_INSTALLMENT")?>";
			dojo.byId('lbl_release').innerHTML="<?php echo $tr->translate("START_DATE")?>";
			dojo.byId('lbl_year').innerHTML='<?php echo $tr->translate("MONTH")?>';
			if(dijit.byId("old_paymentmethod").get("value")==1){amount_principal = dijit.byId("sold_price").get('value');}
			if(isNaN(amount_principal)){amount_principal=0;}if(isNaN(debt_amount)){debt_amount=0;}
			total_collect = amount_principal/debt_amount;
			total_collect = total_collect.toFixed(0);
			if(debt_amount!=0){
				dijit.byId('fixed_payment').attr("value",total_collect);
				paid_before = dijit.byId('paid_before').get('value');
				if(isNaN(paid_before)){paid_before=0;}
				dijit.byId('deposit').attr("value",total_collect-paid_before);
				Balance();
			}
			dijit.byId('interest_rate').attr("value",0);
			dijit.byId('interest_rate').set("readOnly",true);
			dijit.byId('first_payment').attr("value",'<?php echo date("Y-m-d");?>');
			
		}else if(method_id==5){//times
			dijit.byId('release_date').set("readOnly",true);
			dijit.byId('interest_rate').attr("value",0);
			dijit.byId('interest_rate').set("readOnly",true);
			dijit.byId('period').set("readOnly",false);
		}
		else if(method_id==6){//times ដំណាក់កាលមិនថេ			
			dijit.byId('interest_rate').attr('value',0);
			
			$('#identity').val('');
			dijit.byId('first_payment').attr("value",'<?php echo date("Y-m-d");?>');
			dojo.byId('lbl_period').innerHTML="<?php echo $tr->translate("TOTAL_INSTALLMENT")?>";
			dojo.byId('lbl_release').innerHTML="<?php echo $tr->translate("START_DATE")?>";
			if(isNaN(amount_principal)){amount_principal=0;}if(isNaN(debt_amount)){debt_amount=0;}

			total_collect = amount_principal/debt_amount;
			total_collect = total_collect.toFixed(2);
			if(isNaN(total_collect)){
				total_collect=0;
			}
			dojo.byId('lbl_year').innerHTML='<?php echo $tr->translate("MONTH")?>';
			dijit.byId('fixed_payment').attr("value",0);
			dijit.byId('fixed_payment').set("readOnly",true);
			dijit.byId('interest_rate').attr("value",0);
			dijit.byId('interest_rate').set("readOnly",true);
		}
	}
	CheckAttrDeposit();
}
function calCulateFixedPeriod(){
	last_payment = dijit.byId('last_payment').get('value');
	if(isNaN(last_payment)){
		last_payment = 0;
	}
	var total_paid_install = dijit.byId('total_installamount').get('value');
	deposit = dijit.byId('deposit').get('value');
	if(isNaN(deposit)){deposit=0;}
	if(total_paid_install == 0){//មិនមានតារាងរំលស់ខាងក្រោម
		amount_principal = dijit.byId('sold_price').get('value')-last_payment;
	}else{//មានតារាងរំលស់ខាងក្រោម
		amount_principal = dijit.byId('sold_price').get('value')-dijit.byId('total_installamount').get('value')-last_payment;
	}	
	if(dijit.byId("old_paymentmethod").get("value")==1 && total_paid_install!=0 ){
		amount_principal = amount_principal;
	}
	dijit.byId('fixed_payment').set('readOnly',false);
	dojo.byId('lbl_property_price').innerHTML="<?php echo $tr->translate("AMOUNT_GRAICE_PARIOD")?>";
	
	debt_amount = dijit.byId('period').get('value')*<?php echo $install_month;?>;
	
	term = debt_amount ;
	interest_rate=dijit.byId('interest_rate').get('value')/12/(100);
	if(interest_rate==0){
		amount_payment = parseFloat(amount_principal/debt_amount);
		if(amount_payment=='Infinity' || amount_payment=='NaN'){amount_payment=0;}
	}else{
		top_pow = Math.pow((1+interest_rate),term);
		bellow = Math.pow((1+interest_rate),(term))-1;
		amount_payment = (amount_principal*interest_rate*top_pow)/(bellow);
		
		if(isNaN(amount_payment)){
			amount_payment=0;
		}
	}		
	dijit.byId('for_installamount').attr('value',amount_principal);
	amount_payment = Math.round(amount_payment);
	dijit.byId('fixed_payment').attr('value',amount_payment);
	
	if(dijit.byId('install_type').get('value')==2){
		dijit.byId('fixed_payment').attr('value',0);
		dijit.byId("fixed_payment").set("readOnly",false);
	}
}
function calculateDiscount(){
	total_sold = dijit.byId('total_sold').get('value');
	discount_percent = dijit.byId('discount_percent').get('value');
	bdiscount_percent = dijit.byId('bdiscount_percent').get('value');
	if(isNaN(discount_percent)){
		discount_percent=0;
	}
	if(isNaN(bdiscount_percent)){
		bdiscount_percent=0;
	}
	discount_percent=parseFloat(discount_percent)+parseFloat(bdiscount_percent);
	
	bdiscount_fixed = dijit.byId('bdiscount_fixed').get('value');
	discount = dijit.byId('discount').get('value');
	if(isNaN(discount)){
		discount=0;
	}
	if(isNaN(bdiscount_fixed)){
		bdiscount_fixed=0;
	}


	other_discount = dijit.byId('bother_discount').get('value');
	if(isNaN(other_discount)){
		other_discount=0;
	}

	discount = parseFloat(discount)+parseFloat(bdiscount_fixed);
	total_sold = total_sold-discount;
	discount_percent = parseFloat(total_sold*discount_percent/100);
	total_discount = discount_percent+discount+other_discount;
	total_sold = total_sold-discount_percent;
	total_sold = (total_sold-other_discount).toFixed(2);
	//old condiction discount % first
	/*
	discount = parseFloat(discount)+parseFloat(bdiscount_fixed);
	discount_percent = parseInt(total_sold*discount_percent/100);
	total_sold = total_sold-discount_percent-discount-other_discount;
	*/
	
	
	dijit.byId('sold_price').attr('value',total_sold);
	Balance();
}
function Balance(){
	price = dijit.byId('sold_price').get('value');
	deposit =dijit.byId('deposit').get('value');
	paid_before = dijit.byId("paid_before").get("value");
	
	if(isNaN(deposit)){
		deposit=0;
	}
	if((deposit+paid_before)>price){
		infoMessageAlert("ប្រាក់បង់មិនអាចធំជាងតម្លៃលក់បានទេ!");
		dijit.byId('deposit').focus();
		dijit.byId('deposit').attr('value',price-paid_before)
		dijit.byId('balance').attr('value',0)
	}else{ 
		schedule_opt = dijit.byId('schedule_opt').get('value');
		if(schedule_opt==2){
			dijit.byId('balance').attr('value',0);
		}else{
			if(schedule_opt==3){
				dijit.byId('for_installamount').attr('value',price-deposit);
			}
			paid_before = dijit.byId('paid_before').get('value');
			if(isNaN(paid_before)){paid_before=0;}
			dijit.byId('balance').attr('value',price-deposit-paid_before);
		}
	}
