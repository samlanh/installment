function calculateDuration(type=null){
	<?php if($autocalcualte==1){// 1 auto calculate date ,0 no calculate?>
	if(type==1){
		var total_paid_install = dijit.byId('for_installamount').get('value');
		var other_fee = dijit.byId('other_fee').get('value');
		if(isNaN(other_fee)){
			other_fee=0;
		}
		if(total_paid_install == 0){
			sold_price = dijit.byId('sold_price').get('value')+other_fee;
		}else{
			sold_price = (dijit.byId('sold_price').get('value')+other_fee)-dijit.byId('paid_before').get('value');
		}
		
		interest_rate= dijit.byId('interest_rate').get('value')/12/(100);
		fixed_payment = dijit.byId('fixed_payment').get('value');
		if(interest_rate>0){
			if(isNaN(interest_rate)){interest_rate=0;}
			
			pi = (parseFloat(fixed_payment)-(parseFloat(sold_price)*parseFloat(interest_rate)));
			s = parseFloat(fixed_payment);
			top_ii = Math.log(s/pi);
	
			var duration =  Math.ceil(top_ii / Math.log(1+ parseFloat(interest_rate)));// ROUND UP
		}else if(interest_rate==0){
			duration = Math.round(sold_price/fixed_payment);
		}
		dijit.byId('period').attr('value',duration);
	}
	<?php }?>
}
