	price = dijit.byId('sold_price').get('value');
	paid_before = dijit.byId('paid_before').get('value');
	
	if(isNaN(paid_before)){
		paid_before=0;
	}
	other_fee = dijit.byId('other_fee').get('value');
	if(isNaN(other_fee)){
		other_fee=0;
	}
	price = price-paid_before+other_fee;	
	price = parseFloat(price).toFixed(2);
	schedule_opt = dijit.byId('schedule_opt').get('value');
