var	url_getSaleInfo = '<?php echo $this->url(array('module'=>'issue','controller'=>'receivplong','action'=>'getsaleinfo'));?>';
function getSaleInfo(){
	plong_type = dijit.byId('plong_type').get('value');
	if(plong_type==-1 || plong_type==""){ infoMessageAlert("<?php echo $tr->translate("PLEASE_LAYOUT_TYPE")?>"); dijit.byId('plong_type').focus();return false;}
